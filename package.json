{
  "name": "siiimple-toast",
  "version": "2.0.0",
  "description": "easily replace console.log and alert in toast message inspired by material design",
  "main": "./dist/main.js",
  "scripts": {    
    "src-build": "babel src --presets babel-preset-es2015 --plugins transform-object-assign --out-dir dist",
    "public-build": "babel public --presets babel-preset-es2015 --plugins transform-object-assign --out-dir dist",
    "build": "npm run src-build && npm run public-build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gomonk3037/siiimple-Toast.git"
  },
  "keywords": [
    "toast",
    "alert",
    "message",
    "consolelog"
  ],
  "author": "GoEunseok",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gomonk3037/siiimple-Toast/issues"
  },
  "homepage": "https://github.com/gomonk3037/siiimple-Toast#readme",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "eslint": "^4.0.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.3.0"
  }
}
